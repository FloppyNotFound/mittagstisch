{
  "general": {
    "author": "Marcel Jänicke",
    "description": "The lunch in your area. Based on Leipzig (Plagwitz, Lindenau).",
    "github": {
      "use": true,
      "username": "inpercima"
    },
    "name": "mittagstisch",
    "useDocker": false
  },
  "client": {
    "buildDir": "dist",
    "installDependencies": true,
    "packageJson": {
      "contributors": [{
        "name": "Marcel Jänicke",
        "email": "inpercima@gmail.com"
      }],
      "homepage": "https://github.com/inpercima/mittagstisch",
      "repository": "https://github.com/inpercima/mittagstisch"
    },
    "route": {
      "features": {
        "default": "dashboard",
        "show": true
      },
      "login": {
        "activate": false,
        "name": "login",
        "show": false
      },
      "notFound": {
        "name": "not-found",
        "redirect": false
      }
    },
    "selectorPrefix": "mt",
    "theme": "indigo-pink",
    "title": "Mittagstisch",
    "useYarn": true
  },
  "server": {
    "backend": "java",
    "htaccess": false,
    "management": "maven",
    "packagePath": "net.inpercima.mittagstisch",
    "serverAsApi": false
  }
}
